{
    "name": "craftsnippets/modal-component",
    "description": "Modal component plugin for Craft CMS",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "modal component",
        "modal"
    ],
    "support": {
        "docs": "https://github.com/craft-snippets/Craft-modal-component/blob/master/README.md",
        "issues": "https://github.com/craft-snippets/Craft-modal-component/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Pogorzelski",
            "homepage": "https://craftsnippets.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^5.0.0"
    },
    "autoload": {
        "psr-4": {
          "craftsnippets\\modalcomponent\\": "src/"
        }
    },
    "extra": {
        "name": "Modal component",
        "handle": "modal-component",
        "developer": "Piotr Pogorzelski",
        "developerUrl": "https://craftsnippets.com/",
        "documentationUrl": "https://github.com/craft-snippets/Craft-modal-component/blob/master/README.md",
        "changelogUrl": "https://github.com/craft-snippets/Craft-modal-component/blob/master/CHANGELOG.md",
        "class": "craftsnippets\\modalcomponent\\ModalComponent"
    }
